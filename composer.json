{
    "name": "prs1378/tickets-filament",
    "description": "Ticket system plugin for FilamentPHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "prs1378\\FilamentTickets\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "livewire/livewire": "^3.0",
        "pusher/pusher-php-server": "^7.0",
        "prs1378/ticket-filament": "dev-main"
    },
    "extra": {
        "laravel": {
            "providers": [
                "prs1378\\FilamentTickets\\FilamentTicketsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "vendor/prs1378/ticket-filament"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
